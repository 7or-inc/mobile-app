{
  "expo": {
    "name": "7or",
    "slug": "7or",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/7or-logo.png",
    "scheme": "7or",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "assets": ["./src/assets/fonts/"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/7or-logo.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/7or-logo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/7or-logo.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-localization"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "c138b367-bbaa-4273-887e-01d4f0dfb9fb"
      }
    },
    "owner": "omarlawaty"
  }
}
